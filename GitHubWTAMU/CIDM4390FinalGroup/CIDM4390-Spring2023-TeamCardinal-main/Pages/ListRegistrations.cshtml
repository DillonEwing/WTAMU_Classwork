@page
@model ListRegistrations
@{
    ViewData["Title"] = "List Registrations";
}

<h1>@ViewData["Title"]</h1>

<table class="table table-sm table-bordered">
    <tr><th>Session</th><th>Edit</th></tr>
    @foreach (var reg in Model.RegList)
    {
        <tr>
            <td>@reg.Text</td>
            <td>
                <a asp-page="./EditSurvey" asp-route-id="@reg.Value">Edit</a>
            </td>
        </tr>
    }
</table>